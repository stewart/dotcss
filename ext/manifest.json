{
  "name"             : "dotcss",
  "manifest_version" : 2,
  "version"          : "2.2.0",
  "description"      : "~/.css",
  "icons": {
     "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [{
    "all_frames" : true,
    "run_at"     : "document_start",
    "matches"    : ["http://*/*", "https://*/*"],
    "js"         : ["dotcss.js"]
  }],
  "permissions": ["tabs"]
}
